{
  "name": "action-items",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "env-cmd -f ./.env.development.local react-app-rewired build --REACT_APP_ENV development",
    "build:qa": "env-cmd -f ./.env.qa.local react-app-rewired build --REACT_APP_ENV qa ",
    "build:staging": "env-cmd -f ./.env.staging.local react-app-rewired build --REACT_APP_ENV staging ",
    "build:prod": "env-cmd -f ./.env.production.local react-app-rewired build --REACT_APP_ENV production ",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
